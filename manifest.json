{
  "manifest_version": 2,
  "name": "Envious",
  "version": "1.3",
  "description": "Replace matching URL requests from one domain to another.",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "storage", "webRequestBlocking", "webRequest", "tabs", "*://*/*"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "icons": { "128": "icon-128.png" },
  "page_action": {
    "default_icon": {
      "32": "icon-32.png"
    },
    "default_title": "Envious",
    "default_popup": "popup.html"
  }
}
